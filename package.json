{
  "name": "exam-management",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "prestart": "npm install",
    "start": "node server.js 8080",
    "dev": "nodemon server.js 8080",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "up": "npx sequelize-cli db:migrate",
    "undo": "npx sequelize-cli db:migrate:undo",
    "undo:all": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo",
    "seed:undo:all": "npx sequelize-cli db:seed:undo:all",
    "status": "npx sequelize-cli db:migrate:status",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.0",
    "nodmeon": "^0.0.1-security",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2"
  }
}
